"generated":{"line":99,"column":63},"source":"src/js/views/RecipeView.js","original":{"line":174,"column":15}}],"sources":{"src/js/views/RecipeView.js":"import icons from './../../img/icons.svg';\r\n\r\nclass RecipeView {\r\n    \r\n    constructor(parentElement) {\r\n        this.parentElement = parentElement;\r\n        this.errorMessage = 'We could not find that recipe. Please try another one!';\r\n    }\r\n\r\n    _clearHTML() {\r\n        this.parentElement.innerHTML = '';\r\n    }\r\n\r\n    _generateSpinner () {\r\n       return  `\r\n        <div class=\"spinner\">\r\n        <svg>\r\n          <use href=\"${icons}#icon-loader\"></use>\r\n        </svg>\r\n        </div> `;\r\n    }\r\n\r\n    _generetaErrorMessage() {\r\n      return `<div class=\"error\">\r\n      <div>\r\n        <svg>\r\n          <use href=\"${icons}#icon-alert-triangle\"></use>\r\n        </svg>\r\n      </div>\r\n      <p>${this.errorMessage}</p>\r\n    </div>`\r\n    }\r\n\r\n    _addToDOM(element) {\r\n        this._clearHTML();\r\n        this.parentElement.insertAdjacentHTML('afterbegin',element);\r\n    }\r\n\r\n    _generateMarkup() {\r\n       const {publisher, ingredients, source_url: sourceUrl, image_url: imageURL, title, cooking_time: cookingTime, servings} = this._data;\r\n      return `\r\n   <figure class=\"recipe__fig\">\r\n     <img src=\"${imageURL}\" alt=\"Tomato\" class=\"recipe__img\" />\r\n     <h1 class=\"recipe__title\">\r\n       <span>${title}</span>\r\n     </h1>\r\n   </figure>\r\n\r\n   <div class=\"recipe__details\">\r\n     <div class=\"recipe__info\">\r\n       <svg class=\"recipe__info-icon\">\r\n         <use href=\"${icons}#icon-clock\"></use>\r\n       </svg>\r\n       <span class=\"recipe__info-data recipe__info-data--minutes\">${cookingTime}</span>\r\n       <span class=\"recipe__info-text\">minutes</span>\r\n     </div>\r\n     <div class=\"recipe__info\">\r\n       <svg class=\"recipe__info-icon\">\r\n         <use href=\"${icons}#icon-users\"></use>\r\n       </svg>\r\n       <span class=\"recipe__info-data recipe__info-data--people\">${servings}</span>\r\n       <span class=\"recipe__info-text\">servings</span>\r\n\r\n       <div class=\"recipe__info-buttons\">\r\n         <button class=\"btn--tiny btn--increase-servings\">\r\n           <svg>\r\n             <use href=\"${icons}#icon-minus-circle\"></use>\r\n           </svg>\r\n         </button>\r\n         <button class=\"btn--tiny btn--increase-servings\">\r\n           <svg>\r\n             <use href=\"${icons}#icon-plus-circle\"></use>\r\n           </svg>\r\n         </button>\r\n       </div>\r\n     </div>\r\n\r\n     <div class=\"recipe__user-generated\">\r\n       <svg>\r\n         <use href=\"${icons}#icon-user\"></use>\r\n       </svg>\r\n     </div>\r\n     <button class=\"btn--round\">\r\n       <svg class=\"\">\r\n         <use href=\"${icons}#icon-bookmark-fill\"></use>\r\n       </svg>\r\n     </button>\r\n   </div>\r\n\r\n   <div class=\"recipe__ingredients\">\r\n     <h2 class=\"heading--2\">Recipe ingredients</h2>\r\n     <ul class=\"recipe__ingredient-list\">\r\n       ${ingredients.map(ing => {\r\n      \r\n         return `<li class=\"recipe__ingredient\">\r\n         <svg class=\"recipe__icon\">\r\n           <use href=\"${icons}#icon-check\"></use>\r\n         </svg>\r\n         <div class=\"recipe__quantity\">${ing.quantity === null ? '' : ing.quantity}</div>\r\n         <div class=\"recipe__description\">\r\n           <span class=\"recipe__unit\">${ing.unit}</span>\r\n           ${ing.description}\r\n         </div>\r\n       </li>`;\r\n         \r\n       ;}).join('')}\r\n\r\n\r\n       \r\n\r\n       <li class=\"recipe__ingredient\">\r\n         <svg class=\"recipe__icon\">\r\n           <use href=\"${icons}#icon-check\"></use>\r\n         </svg>\r\n         <div class=\"recipe__quantity\">0.5</div>\r\n         <div class=\"recipe__description\">\r\n           <span class=\"recipe__unit\">cup</span>\r\n           ricotta cheese\r\n         </div>\r\n       </li>\r\n     </ul>\r\n   </div>;\r\n\r\n   <div class=\"recipe__directions\">\r\n         <h2 class=\"heading--2\">How to cook it</h2>\r\n         <p class=\"recipe__directions-text\">\r\n           This recipe was carefully designed and tested by\r\n           <span class=\"recipe__publisher\">${publisher}</span>. Please check out\r\n           directions at their website.\r\n         </p>\r\n         <a\r\n           class=\"btn--small recipe__btn\"\r\n           href=\"${sourceUrl}\"\r\n           target=\"_blank\"\r\n         >\r\n           <span>Directions</span>\r\n           <svg class=\"search__icon\">\r\n             <use href=\"${icons}#icon-arrow-right\"></use>\r\n           </svg>\r\n         </a>\r\n       </div>\r\n   `;\r\n   }\r\n\r\n    render(data) {\r\n      if (!data || (Array.isArray(data) && data.length === 0)) {\r\n        return this.renderError();\r\n      }\r\n        \r\n      \r\n      this._data = data;\r\n      \r\n\r\n      const markup = this._generateMarkup();\r\n      this._addToDOM(markup);\r\n    }\r\n\r\n    renderSpinner() {\r\n      this._addToDOM(this._generateSpinner());\r\n    }\r\n\r\n    renderError(){\r\n      this._addToDOM(this._generetaErrorMessage());\r\n    }\r\n\r\n    addHandlerRender(handler) {\r\n      [`hashchange`, 'load'].forEach(event => window.addEventListener(event, handler));\r\n    }\r\n    \r\n}\r\n\r\n\r\n\r\nexport default new RecipeView(document.querySelector('.recipe'));"},"lineCount":null}},"error":null,"hash":"fb7d479be0d7bf8741168442bdc6797b","cacheData":{"env":{}}}